{
  "name": "@koopjs/provider-file-geojson",
  "version": "1.0.1",
  "description": "Simple provider for serving file-based GeoJSON",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "buble -i src -o dist",
    "package": "npm run clean && npm run compile",
    "start": "node server.js",
    "test": "standard && tape test/*.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koopjs/koop-provider-file-geojson.git"
  },
  "author": "Rich Gwozdz",
  "license": "Apache-2.0",
  "devDependencies": {
    "buble": "^0.19.6",
    "proxyquire": "^2.1.0",
    "standard": "^11.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.1"
  },
  "bugs": {
    "url": "https://github.com/koopjs/koop-provider-file-geojson/issues"
  },
  "homepage": "https://github.com/koopjs/koop-provider-file-geojson#readme",
  "dependencies": {
    "koop": "^3.7.0"
  }
}
